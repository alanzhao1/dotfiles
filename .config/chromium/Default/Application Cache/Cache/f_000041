<script type="text/javascript">
    var gotGSStyle = false,
        tmpCache = {};

    (function(root, undefined) {
       var getXHR = (function() {
           // need the try..catch for XHR
           try {
               var defaultReq = XMLHttpRequest;
               new defaultReq();
               return function() { return new defaultReq(); };
           } catch (e) {
               return function() { return new ActiveXObject('Microsoft.XMLHTTP'); };
           }
       })();

       var noop = function() {},
           slice = Array.prototype.slice;

       var miniDfd = function() {
           var pass = [], fail = [], _this = this, _r = 'resolved', _rj = 'rejected', _p = 'pending',
               state = _p, data;
           var change = function(newState, args) {
               if (state != _p) return _this;
               var i = 0, l, callbacks;
               data = slice.call(args, 0);
               state = newState;
               if (state == _r) {
                   callbacks = pass;
               } else {
                   callbacks = fail;
               }
               for (l = callbacks.length; i < l; i++) {
                   callbacks[i].apply(_this, data);
               }
               pass = fail = [];
               return _this;
           };
           this.resolve = function() { return change(_r, arguments); };
           this.reject = function() { return change(_rj, arguments); };
           this.done = function(func) {
               if (state == _p) pass.push(func);
               if (state == _r) func.apply(_this, data);
               return _this;
           };
           this.fail = function(func) {
               if (state == _p) fail.push(func);
               if (state == _rj) func.apply(_this, data);
               return _this;
           };
           this.then = function(func, func2) {
               if (func) _this.done(func);
               if (func2) _this.fail(func2);
               return _this;
           };
           this.state = function() { return state; };
       };

       root.gsPreloadAjax = function(opts) {
           var method = (opts.method || 'GET').toUpperCase(),
               url = opts.url,
               data = opts.data,
               dfd = new miniDfd,
               xhr = getXHR(),
               body = data;

           if (method == 'GET' && data) {
               url = url + "?" + data;
               body = '';
           }

           var callback = function() {
               var response = null, status = 0;
               try {
                   if (xhr.readyState != 4) {
                       return;
                   }
                   response = xhr.responseText;
                   status = xhr.status;
                   if (status == 1223) status = 204;
                   if (response && status && status - 200 < 100) {
                       dfd.resolve(response, status, xhr);
                       return;
                   }
               } catch (e) {}
               dfd.reject(xhr, status, response);
           };

           xhr.open(method, url, true);
           if (method == 'POST') {
               xhr.setRequestHeader('Content-Type', 'application/x-www-form-urlencoded');
           }
           xhr.send(body || null);

           if (xhr.readyState == 4) {
               setTimeout(callback, 10);
           } else {
               xhr.onreadystatechange = callback;
           }

           dfd.xhr = xhr;
           return dfd;
       };
   })(window);


    (function(root) {


    var userAgent = (navigator.userAgent || ''),
        userAgentLower = userAgent.toLowerCase(),
        isMSIE = /(msie) ([\w.]+)/.exec(userAgentLower),
        clientPage = encodeURIComponent(root.GS.loadHash.substr(2)),
        supportsLS = false;
    try {
        supportsLS = window.localStorage !== undefined;
    } catch (e) {}
    function lsGet(key) {
        try {
            return window.localStorage[key];
        } catch (e) {}
        return null;
    }

    root.gsAddCSS = function(url, opts) {
        var doc = document,
            legacy = false,
            stylesheets = doc.styleSheets,
            interval = null,
            loaded = false,
            options = opts || {};

        function onGSStyleLoad() {
            if (loaded) return;
            loaded = true;
            clearInterval(interval);
            interval = null;
            if (options.callback) {
                options.callback();
            }
        }
        function checkGSStyle(index) {
            try {
                if (stylesheets[index].cssRules || (stylesheets[index].rules && stylesheets[index].rules.length)) {
                    onGSStyleLoad();
                    return;
                }
            } catch(e) {}
            if (interval === null && !gotGSStyle) {
                interval = setInterval(function() {
                    checkGSStyle(index);
                }, 20);
            }
        }

        var cssIndex = doc.styleSheets.length - 1,
            sp = document.getElementsByTagName('head')[0] || document.getElementsByTagName('script')[0].parentNode,
            legacyVersion;
        if (legacyVersion = /Firefox[\/\s](\d+\.\d+)/.exec(userAgent)) {
            if (legacyVersion[1] - 0 < 9) {
                legacy = true;
            }
        }
        if (/WebKit[\/\s](\d+\.\d+)/.test(userAgent) && !(/Chrome[\/\s](\d+\.\d+)/.test(userAgent))) {
            legacyVersion = /WebKit[\/\s](\d+\.\d+)/.exec(userAgent);
            if (legacyVersion && legacyVersion[1] - 0 < 536) {
                legacy = true;
            }
        }

        var style, css;
        if (legacy) {
            style = doc.createElement("style");
            style.type = "text/css";
            style.textContent = ['@import url("', url, '");'].join('');
            sp.appendChild(style);
        } else {
            css = doc.createElement("link");
            css.rel = "stylesheet";
            css.type = "text/css";
            css.async = 'true';
            css.media = 'only x';
            css.onload = onGSStyleLoad;
            css.onreadystatechange = function () { if (this.readyState == 'complete' || this.readyState == 'loaded') onGSStyleLoad(); };
            css.href = url;
            sp.appendChild(css);
            setTimeout(function() {
                css.media = 'all';
            }, 10);
        }
        checkGSStyle(cssIndex + 1);

        return css || style;
    };
    root.gsAddScript = function(url, opts) {
        var doc = document,
            newScript = doc.createElement('script'),
            s = doc.getElementsByTagName('script')[0],
            sp = s.parentNode,
            options = opts || {},
            ran = false,
            retryNoCors = false;

        newScript.type = 'text/javascript';
        newScript.async = 'true';
        if (options.cors && !lsGet('failedCors')) {
            newScript.crossOrigin = 'anonymous';
            if (options.retryCors) {
                retryNoCors = true;
            }
        }
        if (options.id) {
            newScript.id = options.id;
        }
        if (options.callback) {
            var once = function(ev) {
                if (isMSIE && this.readyState && this.readyState !== "complete" && this.readyState !== "loaded") {
                    return;
                }
                if (ran) return;
                ran = true;
                if (ev && ev.type === 'error' && retryNoCors) {
                    options.cors = false;
                    options.disabledCors = true;
                    newScript.remove();
                    root.gsAddScript(url, options);
                } else if (ev && ev.type === 'load' && options.disabledCors === true && supportsLS) {
                    window.localStorage.failedCors = true;
                    root.GSLogger && root.GSLogger.notify('Failed to load script with cors and recovered without cors', 'WARNING');
                }
                options.callback.call(this);
            };
            newScript.onload = newScript.onreadystatechange = newScript.onerror = once;
        }
        newScript.src = url;
        sp.insertBefore(newScript, s);
        return newScript;
    };
    
                var gsStyleURL = 'http://static-a.gs-cdn.net' + (root.dataURISupport === false ? '/static/production_20150424125252.css' : '/static/production-uri_20150424125252.css');
        
    setTimeout(function() {
        var gsStyleCallback = function() {
                if (!root.gotGSStyle) {
                    root.gotGSStyle = true;
                    if (GS && GS.trigger) {
                        GS.trigger('gsStyle:ready');
                    }
                }
            };
        gsAddCSS(gsStyleURL, {callback: gsStyleCallback});
    }, 10);
    

        if (root.parentSandboxBridge) {
            function getStylesheet(uri) {
                root.parentSandboxBridge.consoleWarn(uri);
                document.write('<link href="'+uri+'" rel="stylesheet" type="text/css" >');
            }


            if (root.gsProduction && root.gsProduction.LB_CSS) {
                if (gsProduction.LB_CSS.assetPath) {
                    getStylesheet(gsProduction.LB_CSS.assetPath);
                }
            }
        }



        var img = new Image(),
            ts = +(new Date()),
            xsd = "https://" + root.location.host + "/crossdomain.xml",
            coreURI;

        gsPreloadAjax({url: '/preload.php?getCommunicationToken=1&hash=' + clientPage + '&' + ts}).done(function(data) {
            Function(data)();
            //now that we got gsConfig from preload, re-init GSLogger
            root.GSLogger && root.GSLogger.initFromIndex();
        }).fail(function(xhr, status) {
            root.GSLogger && root.GSLogger.notify('Failed to load preload.php?getCommunicationToken', 'ERROR', {status: status});
        });
        gsPreloadAjax({url: '/preload.php?hash=' + clientPage + '&' + ts}).done(function(data) {
            window.preloadedDataTotalTime = +(new Date()) - ts;
            window.preloadedDataTotalSize = (data || '').length;
            Function(data)();
        }).fail(function(xhr, status) {
            root.GSLogger && root.GSLogger.notify('Failed to load preload.php', 'ERROR', {status: status});
        });

        if (/(msie) ([\w.]+)/.exec(userAgentLower)) {
            coreURI = '/static/core-compat_20150424125252.js';
        } else {
            coreURI = '/static/core_20150424125252.js';
        }

        var coreURL = 'http://static-a.gs-cdn.net' + coreURI,
            appURL = 'http://static-a.gs-cdn.net/static/app_20150424143246.js',
            gaURL = '//www.google-analytics.com/analytics.js',
            csURL = 'http://grooveshark.com/webincludes/js/beacon.js',
            coreFailed = false, appFailed = false, timeout = 0;
        gsAddScript(coreURL, {id: 'coreJS', cors: true, retryCors: true, callback: function() {
            if (!coreFailed && root.GSLogger && !root.groovesharkCore) {
                //try to wait for script to be processed
                setTimeout(function() {
                    if (!root.groovesharkCore) {
                        root.GSLogger.notify('Failed to load/parse core.js', 'ERROR');
                        coreFailed = true;
                    }
                    clearTimeout(timeout);
                }, 300);
            } else if (root.gsBootstrap) {
                clearTimeout(timeout);
            }
        }});
        gsAddScript(appURL, {cors: true, retryCors: true, callback: function() {
            if (!coreFailed && root.GS && root.GSLogger && !root.gsBootstrap) {
                //try to wait for script to be processed
                setTimeout(function() {
                    if (!root.gsBootstrap) {
                        root.GSLogger.notify('Failed to load/parse app.js', 'ERROR');
                        appFailed = true;
                    }
                    clearTimeout(timeout);
                }, 300);
            } else if (root.gsBootstrap) {
                clearTimeout(timeout);
            }
        }});
        if (root.GSLogger && !appFailed) {
            function onFailedLoad() {
                var sinceLoad = +(new Date()) - ts;
                if (!coreFailed && !appFailed && sinceLoad > 5000 && !root.gsBootstrap) {
                    root.GSLogger.notify('Site never loaded', 'WARNING', {secs: Math.round(sinceLoad / 1000)}, true);
                    appFailed = true;
                }
            }
            window.onbeforeunload = onFailedLoad;
            timeout = setTimeout(onFailedLoad, 15000);
        }

    
        (function(window) {
            //Small amount of GA setup for Universal Analytics
            window.GoogleAnalyticsObject = 'ga';
            window.ga = window.ga || function() {
                (window.ga.q = window.ga.q || []).push(arguments);
            };
            window.ga.l = 1 * new Date();
        })(window);
        gsAddScript(gaURL); // google analytics
        gsAddScript(csURL); // comScore

        var readCookieKey = function(name) {
            var cookies = document.cookie.split(";"),
                i = 0, l = cookies.length,
                namePreSpace = ' ' + name,
                namePostSpace = name + ' ',
                nameSpaces = namePreSpace + ' ',
                n, v, s;
            for (; i < l; i++) {
                s = cookies[i].indexOf("=");
                n = cookies[i].substr(0, s);
                v = cookies[i].substr(s + 1);
                if (n === name || n === namePreSpace || n === namePostSpace || n === nameSpaces) {
                    return unescape(v);
                }
            }
        };

        // Initial GA pageview is done in the router
        var overrideReferrer = readCookieKey('referrerURL');
        ga('create', 'UA-1894124-3', 'auto');
        ga('set', 'referrer', overrideReferrer);
        ga('require', 'displayfeatures');

    

    
        tmpCache[xsd] = img;
        img.onload = img.onerror = function() { delete tmpCache[xsd]; };
        img.src = xsd;
        img = null;
    

    })(window);

</script>


